package keben;
import java.util.Scanner;
public class I {
    public static void main (String[]args) {
     Scanner s=new Scanner(System.in);
     System.out.println("请输入一个数");
     int a= s.nextInt();
     int b=1;  
     for(;b<a;b++){
    		 if(a%b==0){
    			System.out.println("因子的值为"+b);
    		 }
    		 int sum=0;
    		 sum=sum+b;
    		 System.out.println("因子的和为"+sum);
     }
    }
}